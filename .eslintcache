pp\">\n      <div className=\"main\">\n        <Switch>\n\n          <Route exact path=\"/\" render={() => (\n            <HomepageLayout>\n              <Homepage />\n            </HomepageLayout>\n          )} />\n\n          <Route path=\"/registration\" render={() => currentUser ? <Redirect to=\"/\" /> : (\n            <MainLayout>\n              <Registration />\n            </MainLayout>\n          )} />\n\n          <Route path=\"/login\"\n          render={() => currentUser ? <Redirect to=\"/\" /> : (\n            <MainLayout>\n              <Login />\n            </MainLayout>\n          )} />\n\n          <Route path=\"/recovery\" render={() => (\n            <MainLayout>\n              <Recovery />\n            </MainLayout>\n          )} />\n\n\n        </Switch>\n      </div>\n    </div>\n  );\n  \n}\n\nconst mapStateToProps = ({user}) => ({\n  currentUser: user.currentUser\n})\n\nconst mapDispatchToProps = dispatch => ({\n  setCurrentUser: user => dispatch(setCurrentUser(user))\n})\n\nexport default connect(mapStateToProps,mapDispatchToProps)(App);\n ","D:\\0learn\\recommerce\\riyek\\src\\pages\\Homepage\\index.js",[],"D:\\0learn\\recommerce\\riyek\\src\\components\\Header\\index.js",[],"D:\\0learn\\recommerce\\riyek\\src\\components\\Directory\\index.js",["104","105"],"import satu from '../../assets/amwa1.jpeg';\nimport dua from '../../assets/amwa2.jpeg';\n\nimport './styles.scss'\n\nconst Directory = props => {\n    return (\n        <div className=\"directory\">\n            <div className=\"wrap\">\n                <div className=\"item\"\n                    style={{ backgroundImage: `url(${satu})` }}\n                >\n                    <a>Shope Satu</a>\n                </div>\n                <div\n                    className=\"item\"\n                    style={{ backgroundImage: `url(${dua})` }}\n                >\n                    <a>Shope Dua</a>\n                </div>\n            </div>\n\n        </div>\n    )\n};\n\nexport defa